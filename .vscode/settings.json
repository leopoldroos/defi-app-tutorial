{
    "editor.scrollBeyondLastLine": false,
    "vsicons.presets.angular": false,
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "dist": true,
        "cache": true,
        // "node_modules": true,
        "bower": true,
        "bower_components": true,
        "/output/": true,
        "**/*.js.map": true,
        "**/bundle-*": true,
        "**/chunk*": true,
        "**/*.cache": true,
        "/.cache/:true": true
